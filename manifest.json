{
  "manifest_version": 3,
  "name": "Video Speed Controller",
  "version": "1.0",
  "description": "Control video speed",
  "icons": {
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content/content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "permissions": ["storage"]
}